srecord/.libdir:
	-$(INSTALL) -m 0755 -d $(libdir)
	@-test -d $(libdir) && touch $@
	@sleep 1

srecord/libsrecord.la: $(lib_obj)
	rm -f $@
	$(LIBTOOL) --mode=link --tag=CXX $(CXX) $(CPPFLAGS) $(CXXFLAGS) -o $@ \
		$(lib_obj) $(LDFLAGS) $(LIBS) -rpath $(libdir) -version-info \
		0:0:0

$(libdir)/libsrecord.la: srecord/libsrecord.la srecord/.libdir
	$(LIBTOOL) --mode=install $(INSTALL_DATA) srecord/libsrecord.la $@

srec_cat_obj = srec_cat/arglex3.lo srec_cat/main.lo

bin/srec_cat: $(srec_cat_obj) srecord/libsrecord.la .bin
	$(LIBTOOL) --mode=link --tag=CXX $(CXX) $(CPPFLAGS) $(CXXFLAGS) -o $@ \
		$(srec_cat_obj) srecord/libsrecord.la $(LDFLAGS) $(LIBS)

$(bindir)/srec_cat: bin/srec_cat .bindir
	$(LIBTOOL) --mode=install $(INSTALL_PROGRAM) bin/srec_cat $@

